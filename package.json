{
  "name": "socketeer",
  "author": "Mimemat <hyatomimemat@gmail.com>",
  "version": "1.0.0",
  "description": "A GUI for websockets",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack/react.webpack.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "cross-env NODE_ENV=production webpack --config webpack/electron.webpack.js",
    "build:react": "cross-env NODE_ENV=production webpack --config webpack/react.webpack.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder",
    "test": "jest",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@commitlint/core": "^9.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-modal": "^3.12.0",
    "@types/react-resizable": "^1.7.2",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "electron": "11.2.1",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "26.6.3",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-refresh": "^0.9.0",
    "ts-jest": "26.4.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^3.9.5",
    "wait-on": "^5.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@unform/core": "^2.1.5",
    "@unform/web": "^2.1.5",
    "electron-store": "^6.0.1",
    "polished": "^4.0.5",
    "react": "17.0.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "react-modal": "^3.12.1",
    "react-resizable": "^1.11.0",
    "react-spring": "^8.0.27",
    "recoil": "^0.1.2",
    "socket.io": "^3.1.0",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.2",
    "ws": "^7.4.1",
    "yup": "^0.32.9"
  },
  "build": {
    "appId": "com.mimemat.socketeer",
    "productName": "Socketeer",
    "mac": {
      "icon": "build/icon.png",
      "category": "public.app-category.productivity"
    },
    "win": {
      "icon": "build/icon.png",
      "target": "nsis",
      "publish": [
          "github"
      ]
    },
    "linux": {
      "icon": "build/icon.png",
      "target": [
          "deb",
          "AppImage"
      ]
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
